{
  "name": "canvas-task-manager",
  "version": "1.0.0",
  "description": "Personal calendar with Canvas LMS integration",
  "main": "server/index.js",
  "scripts": {
    "setup": "npm install && cd client && npm install",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node server/index.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "db:init": "node server/db/init.js",
    "start": "NODE_ENV=production node server/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@clerk/express": "^1.7.62",
    "@gfazioli/mantine-onboarding-tour": "^2.6.6",
    "@libsql/client": "^0.17.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
